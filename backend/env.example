# Configuration de l'environnement - Hunger-Talk Backend
# Copiez ce fichier en .env et remplissez les valeurs

# Base de données PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/hungertalk_db

# JWT Secret Key (générez une clé aléatoire)
SECRET_KEY=votre_cle_secrete_jwt_ici_changez_moi
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configuration Ollama (IA locale)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1:8b

# Configuration CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080

# Environnement
ENVIRONMENT=development
DEBUG=True

# Configuration de l'API OpenFoodFacts (optionnel)
OPENFOODFACTS_API_URL=https://world.openfoodfacts.org/api/v0

